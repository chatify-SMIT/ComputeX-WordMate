inputtext1=document.getElementById('inputtexts1');
inputtext2=document.getElementById('inputtexts2');


  document.body.addEventListener('click', function(event) {

  
    // Check if the click event target is the particular div or one of its children
    if (!editor.contains(event.target)) {
      editor.focus();

    }
if(inputtext1.contains(event.target)){
  inputtext1.focus();
}
if(inputtext2.contains(event.target)){
  inputtext2.focus();
}

  });

 